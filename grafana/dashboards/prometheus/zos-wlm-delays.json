          "editorMode": "code",
          "exemplar": false,
          "expr": "topk(20, label_replace({__name__=~\"mvs_wlm_group_delay_(.*)_percent\", group_type=\"W\",sysplex=~\"${sysplex}\",sysname=~\"${sysname}\"}, \"delay\", \"$1\", \"__name__\", \"mvs_wlm_group_delay_(.*)_percent\"))",
